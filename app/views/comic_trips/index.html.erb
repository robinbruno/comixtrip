<div class="carousel-background mt-5">
  <div class="block_carousel mt-4" style="background: $auburn;">
    <div class="container mt-2">
      <div id="carouselExample" class="carousel slide">
        <div class="carousel-inner">
          <div class="carousel-item active">
            <img class="d-block w-75" style="margin-left: auto; margin-right: auto;" src="https://res.cloudinary.com/dlmpwvjpw/image/upload/v1709738256/Placeholder_carousel_3_ojmy2h.png" alt="First slide">
          </div>
          <div class="carousel-item">
            <img class="d-block w-75" style="margin-left: auto; margin-right: auto;" src="https://res.cloudinary.com/dlmpwvjpw/image/upload/v1709738255/Placeholder_carousel_2_j9xwfr.png" alt="Second slide">
          </div>
          <div class="carousel-item">
            <img class="d-block w-75" style="margin-left: auto; margin-right: auto;" src="https://res.cloudinary.com/dlmpwvjpw/image/upload/v1709738255/Placeholder_carousel_1_w9imhd.png" alt="Third slide">
          </div>
          </div>
          <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
          </button>
          <button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
          </button>
      </div>
    </div>
  </div>
</div>
